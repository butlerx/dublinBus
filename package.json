{
  "name": "dublin-bus.js",
  "version": "2.1.0",
  "description": "Library for querying Dublin bus realtime info",
  "main": "index.js",
  "author": "butlerx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/butlerx/dublinBus"
  },
  "bugs": {
    "url": "http://github.com/butlerx/dublinBus/issues"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "babel src --source-maps --out-dir lib",
    "build:watch": "babel src --watch --source-maps --out-dir lib",
    "precommit": "yarn docs && git add API.md && lint-staged ",
    "prepublish": "yarn build",
    "docs": "jsdoc2md --configure jsdoc.json src/**/*.js >API.md"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": ">6.0.0",
    "babel-plugin-jsdoc-to-assert": "^3.0.4",
    "babel-plugin-jsdoc-to-condition": "^0.3.4",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^4.0.1",
    "json-server": "^0.12.1",
    "lint-staged": "^4.0.1",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  }
}
