{
  "name": "dublin-bus.js",
  "version": "2.0.0",
  "description": "Library for querying Dublin bus realtime info",
  "main": "index.js",
  "author": "butlerx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/butlerx/dublinBus"
  },
  "bugs": {
    "url": "http://github.com/butlerx/dublinBus/issues"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "babel src --source-maps --out-dir lib",
    "build:watch": "babel src --watch --source-maps --out-dir lib",
    "precommit": "lint-staged",
    "prepublish": "yarn run build",
    "format": "prettier-eslint --write src/**/*.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "prettier-eslint-cli": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  }
}
