{
  "name": "dublin-bus.js",
  "version": "1.3.1",
  "description": "Library for querying Dublin bus realtime info",
  "main": "lib/app.js",
  "author": "butlerx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/butlerx/dublinBus"
  },
  "bugs": {
    "url": "http://github.com/butlerx/dublinBus/issues"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "babel src --source-maps --out-dir lib",
    "build:watch": "babel src --watch --source-maps --out-dir lib",
    "precommit": "lint-staged",
    "prepublish": "yarn run build",
    "format": "prettier-eslint --write src/**/*.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "^6.23.0",
    "es6-promise": "^4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "prettier-eslint-cli": "^4.1.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  }
}
